params {
    threads=12

    ref = null

    variant_quality_threshold=10
    variant_depth_threshold=50
    low_cov_threshold=5
}

process {
    withName:consensus_nanopore_process {
        container = 'skkujin/consensus_nanopore:latest'
    }
    withName:qualimap {
        container = 'pegi3s/qualimap:latest'
    }
}

docker {
    enabled = true
}
